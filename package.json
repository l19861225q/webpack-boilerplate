{
  "name": "webpack-boilerplate",
  "version": "0.1.0",
  "description": "Webpack Boilerplate",
  "bin": {
    "cross-env": "./node_modules/.bin/cross-env",
    "webpack": "./node_modules/.bin/webpack",
    "webpack-dev-server": "./node_modules/.bin/webpack-dev-server"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.dev.babel.js --progress",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.pro.babel.js --progress"
  },
  "keywords": [
    "webpack",
    "boilerplate"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/l19861225q/webpack-boilerplate"
  },
  "author": "LiuQian <l19861225q@gmail.com> (http://l19861225q.github.io/blog)",
  "license": "MIT",
  "engines": {
    "node": "8.3.0"
  },
  "bugs": {
    "url": "https://github.com/l19861225q/webpack-boilerplate/issues",
    "email": "l19861225q@gmail.com"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.17",
    "compression-webpack-plugin": "^1.1.3",
    "cross-env": "^5.1.3",
    "css-loader": "^0.25.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.7.2",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "htmlhint": "^0.9.13",
    "imagemin-webpack-plugin": "^1.0.8",
    "img-loader": "^1.3.1",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.13.2",
